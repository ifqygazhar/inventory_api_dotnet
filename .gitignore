bin/
obj/
out/
publish/
app.publish/

# Visual Studio / Rider / VS Code
.vs/
.vscode/
.idea/
*.sln.iml
*.user
*.suo

# NuGet
*.nupkg
.nuget/packages/

# Runtime / debug / logs
*.log
TestResults/
*.trx
*.pid

# OS files
.DS_Store
Thumbs.db

# Database files
*.mdf
*.ldf
*.sqlite
*.db
*.db-journal

# Coverage
*.coverage
*.coveragexml
coverage/
coverage*.json

